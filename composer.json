{
    "name": "iandenh/cakephp-sendgrid",
    "description": "SendgridEmail plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "sendgrid"],
    "license": "MIT",
    "homepage": "https://github.com/Iandenh/cakephp-sendgrid",
    "support": {
        "issues": "https://github.com/Iandenh/cakephp-sendgrid/issues",
        "source": "https://github.com/Iandenh/cakephp-sendgrid"
    },
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "SendgridEmail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SendgridEmail\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
